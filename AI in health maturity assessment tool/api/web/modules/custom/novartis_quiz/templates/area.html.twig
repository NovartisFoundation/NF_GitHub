{#
/**
 * @file area.html.twig
 * Default theme implementation to present Area data.
 *
 * This template is used when viewing Area pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_area()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('area') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
